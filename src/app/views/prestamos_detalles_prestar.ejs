<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Handjet:wght@300&family=Roboto&display=swap" rel="stylesheet">
  <link href="/css/style.css" rel="stylesheet" type="text/css">
</head>
<body>
  <div class="titulo">Biblioteca Desarrollo Web</div>
  <nav class="menu navbar navbar-expand-lg navbar-light" >
    <div class="container-fluid">
      <a class="navbar-brand" id="tituloMenu"  href="#">DeW</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item ">
            <a class="nav-link "  id="itemMenu"   href="/">Inicio</a>
          </li>
          <li class="nav-item ">
            <a class="nav-link active" id="itemMenu" aria-current="page" href="#">Socios</a>
          </li>
          <li class="nav-item">
            <a class="nav-link " id="itemMenu" href="/libros">Libros</a>
          </li>
          <li class="nav-item">
            <a class="nav-link "  id="itemMenu"  href="/prestamos">Prestamos</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  
      <div class="contenedor1">
        <div class="contenedorFormulario col-md-6 ">
          <ul class="nav nav-tabs col-md-12">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="">Prestamos</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/prestamos/ejemplares">Ejemplares</a>
            </li>
           
          </ul>

        <div class="col-md-12 card contenedorInterno">
          <div class="col-md-12 mt-4 text-center">
          
            <table class="table table-striped tabla">
                <tr >
                    <th id="encabezadoTabla">Cod</th>
                    <th id="encabezadoTabla">Titulo</th>
                    <th id="encabezadoTabla">CodEjemplar</th>
                    <th id="encabezadoTabla">ISBN</th>
                    <th id="encabezadoTabla">AÃ±o lanzamiento</th>
                </tr>
               
                <tr>
                  <td><%= ejemplar.CodLibro  %></td>
                  <td><%= ejemplar.Titulo  %></td>
                    <td><%= ejemplar.CodEjem  %></td>
                    <td><%= ejemplar.ISBN  %></td>
                    <td><%= ejemplar.AEscritura  %></td>
                    
                                  
                    
                     
                </tr>
              
            </table>
            <form action="/prestamos/libros/detalles/prestar/<%= ejemplar.CodEjem %>" method="post" class="card-body">
              <h3 class="card-title">Socios</h3>
           
              <div class="form-group mt-4">
                <label for="inputState" class="form-label">Autor</label>
                <select name="CodSocio" class="form-select">
                 
                  <% for(var i = 0;i<socios.length;i++){ %>
                    <option value="<%= socios[i].CodSocio %>"><%= socios[i].Apellido+", "+socios[i].Nombre+"   || Cod:"+socios[i].CodSocio %></option>
                  <% } %>
                  
                </select>
                <input type="submit" class="btn btn-primary my-4" value="Prestar">
              </div>
              </form>
          
        </div>  
      </div>
      </div>
    </div>

